{
  "name": "NodeJSCodeChallenge",
  "version": "1.0.0",
  "main": "src/server.ts",
  "author": "Nathan V Marcelino <nathan.v.marcelino@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "multer": "^1.4.3",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.2.34",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --inspect=0.0.0.0:9229 --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli",
    "start:docker": "docker-compose up",
    "stop:docker": "docker-compose down",
    "rebuild:docker": "docker-compose build --no-cache",
    "build": "yarn build:types && yarn build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".js,.ts,.tsx\" --source-maps inline --copy-files"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/express": "^4.17.12",
    "@types/jest": "^27.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.7.6",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.1.0",
    "prettier": "^2.3.0",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  }
}
